program Problem23;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var
  i:integer;

function checkAbundant(a:integer):boolean;
var
  i,sum:integer;
begin
  sum:=0;
  for i := 1 to (a div 2) do
  if a mod i = 0 then
  sum:=sum+i;
  if sum > a then
  checkAbundant:=true
  else checkAbundant:=false;
end;

